package hw243.sallysstash;

class Stack {
    protected char color;
    protected int[] relativeRows;
    protected int[] relativeCols;

    public char getColor() {
        return color;
    }

    public int[] getRelativeRows() {
        return relativeRows;
    }

    public int[] getRelativeCols() {
        return relativeCols;
    }
}
